package com.sunbeam;

public class binarySearch {
	public static int binarySearch(int arr[],int key) {
		int left=0,right=arr.length,mid,comps=0;
		while(left<=right) {
			comps ++;
			mid = (left+right)/2;
			if(key == arr[mid])
				return mid;
			else if(key<arr[mid])
				right=mid
		}
	}

	public static void main(String[] args) {
	

	}

}
